enum IsGay {
    no,
    yes
}

enum Another {
    x, y, z
}

const anyenum you = IsGay.yes;
const anyenum me = IsGay.no;

var anyenum another = Another.x;
another = IsGay.yes;

println(you == me); # false
println(you == IsGay.yes); # true
println(me == IsGay.no); # true

println(another == Another.x); # true
println(another == Another.y); # false
println(another == Another.z); # false

println(another == you); # false
