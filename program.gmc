enum IsGay {
    no,
    yes
}

enum Another {
    x, y, z
}

const anyenum you = IsGay.yes;
const anyenum me = IsGay.no;

const anyenum another = Another.x;

print(you == me); # false
print(you == IsGay.yes); # true
print(me == IsGay.no); # true

print(another == Another.x); # true
print(another == Another.y); # false
print(another == Another.z); # false

print(another == you); # false
